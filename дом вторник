for (int i = 0; i < n; ++i) {
	double b;
	cin >> b;
	cout << "площадь " <<pl(b) << " периметр " <<per(b)<< '\n';
}

//функция возвращает t\f и меняет число на корень из него, если оно >=0, если неудача, то возвращает число неизменным (3 числа)
